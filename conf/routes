
GET      /                                     controllers.MainPage.home

GET      /conferencier                         controllers.MainPage.conferencier
GET      /api/poll                             controllers.MainPage.poll
PUT      /api/update                           controllers.MainPage.update

GET      /:tableId                             controllers.MainPage.participantTable(tableId: String)




# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                          controllers.Assets.at(path="/public", file)
